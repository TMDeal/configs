URxvt.termName: rxvt-unicode-256color
!URxvt.geometry: 100x40
URxvt.depth: 32
URxvt*loginShell: true
URxvt.iso14755: False

URxvt.scrollTtyKeypress: true
URxvt.scrollTtyOutput: true
URxvt.scrollWithBuffer: true
URxvt.skipScroll: true
URxvt.visualBell: false
URxvt.scrollBar: false
URxvt.borderLess: false

Xft.dpi: 96
Xft.antialias: true
Xft.rgba: rgb
Xft.hinting: true
Xft.hintstyle: hintfull

URxvt.font: xft:Anonymous\ Pro:size=13:antialias=true:hinting=true
URxvt.boldFont: xft:Anonymous\ Pro:bold:size=13:antialias=true:hinting=true
URxvt.italicFont: xft:Anonymous\ Pro:italic:size=13:antialias=true:hinting=true
URxvt.boldItalicFont: xft:Anonymous\ Pro:bold:italic:size=13:antialias=true:hinting=true
URxvt.letterSpace: -1

URxvt.keysym.Shift-Control-V: perl:clipboard:paste
URxvt.keysym.Shift-Control-C: perl:clipboard:copy
URxvt.perl-ext-common:default,clipboard
URxvt.clipboard.copycmd:  xclip -i -selection clipboard
URxvt.clipboard.pastecmd: xclip -o -selection clipboard


!URxvt*.transparent: true
!URxvt*.shading: 30
URxvt.background: [70]#000000
!URxvt*background: #121212
URxvt.foreground: #d7d7d7
URxvt.cursorColor: #d7d7d7
URxvt.color0: #393939
URxvt.color1: #ca674a
URxvt.color2: #96a967
URxvt.color3: #d3a94a
URxvt.color4: #5778c1
URxvt.color5: #9c35ac
URxvt.color6: #6eb5f3
URxvt.color7: #a9a9a9
URxvt.color8: #535551
URxvt.color9: #ea2828
URxvt.color10: #87dd32
URxvt.color11: #f7e44d
URxvt.color12: #6f9bca
URxvt.color13: #a97ca4
URxvt.color14: #32dddd
URxvt.color15: #e9e9e7
